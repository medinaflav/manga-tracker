                var next_chapter =  "" ;
                var prev_chapter =  "https://www.scan-vf.net/one_piece/chapitre-1154/14" ;

                var preload_next = 3;
                var preload_back = 2;
                var current_page = 1;

                var base_url = "https://www.scan-vf.net/one_piece/chapitre-1155";

                var initialized = false;
                
                jQuery(document).ready(function () {
                    $('.selectpicker').selectpicker();
                    if ($("div#all").is(":visible"))
                        $("div#all img").unveil(300);

                    // refresh test
                                            preload(current_page);
                                    });

                // refresh test
                                function changePage(value) {
                    tab= Array();
                    tab[0]="https://www.scan-vf.net/one_piece/chapitre-1155";
                    tab[1]="/";
                    tab[2]=value;
                    location.href = tab.join("");
                }
                                
                function nextPage() {
                    // refresh test
                                        nextPageVal = $('#page-list option:selected').next().val();
                    nextChapterVal = $('#chapter-list li.active').prev().val();

                    if (typeof nextPageVal != 'undefined') {
                        changePage(nextPageVal);
                    }
                    else if (typeof nextPageVal == 'undefined'
                            && typeof nextChapterVal != 'undefined') {
                                            } else {
                        alert('Vous êtes dans la dernière page.');
                    }
                                    }

                function prevPage() {
                    // refresh test
                                        prevPageVal = $('#page-list option:selected').prev().val();
                    prevChapterVal = $('#chapter-list li.active').next().val();

                    if (typeof prevPageVal != 'undefined') {
                        changePage(prevPageVal);
                    }
                    else if (typeof prevPageVal == 'undefined'
                            && typeof prevChapterVal != 'undefined') {
                                                location.href = "https://www.scan-vf.net/one_piece/chapitre-1154/14";
                                            } else {
                        alert('Vous êtes dans la première page.');
                    }
                                    }

                function nextChap(){
                    window.location = next_chapter;
                }
                
                function prevChap(){
                    window.location =  "https://www.scan-vf.net/one_piece/chapitre-1154" ;
                }
                
                function preload(id) {
                    var array = [];
                    var arraydata = [];
                    for (i = -preload_back; i < preload_next; i++) {
                        if (id + i >= 0 && id + i < pages.length) {
                            if (pages[(id + i)].external == 0) {
                                array.push('https://www.scan-vf.net/uploads/manga/one_piece/chapters/chapitre-1155/' + pages[(id + i)].page_image);
                            } else {
                                array.push(pages[(id + i)].page_image);
                            }
                            arraydata.push(id + i);
                        }
                    }

                    jQuery.preload(array, {
                        threshold: 40,
                        enforceCache: true,
                        onComplete: function (data) {
                        }
                    });
                }

                function update_numberPanel() {
                    $('#page-list').selectpicker('val', current_page);
                    $('.pagenumber').text(current_page);
                }

                $('a#modePPP').click(function (e) {
                    e.preventDefault();
                    $('.pager-cnt .page-nav').show();
                    $('div#ppp').show();
                    $('div#all').hide();
                    $(document).on('keyup', function (e) {
                        KeyCheck(e);
                    });
                });

                $('a#modeALL').click(function (e) {
                    e.preventDefault();
                    $('.pager-cnt .page-nav').hide();
                    $('div#ppp').hide();
                    $('div#all').show();
                    $(document).off('keyup');
                    $("div#all img").unveil(300);
                });

                $('select#page-list').on('change', function () {
                    changePage(this.value);
                });

                $(document).on('keyup', function (e) {
                    KeyCheck(e);
                });

                function KeyCheck(e) {
                    var ev = e || window.event;
                    ev.preventDefault();
                    var KeyID = ev.keyCode;
                    switch (KeyID) {
                        case 36:
                            window.location = "https://www.scan-vf.net/one_piece";
                            break;
                        case 33:
                        case 37:
                            prevPage();
                            break;
                        case 34:
                        case 39:
                            nextPage();
                            break;
                    }
                }
                
                                    $('a.bookmark').click(function (e) {
                        e.preventDefault();
                        $.ajax({
                            url: "https://www.scan-vf.net/bookmark",
                            method: 'POST',
                            data: {
                                'manga_id': '1',
                                'chapter_id': '8417',
                                'page_slug': '1',
                                '_token': 'rV8m7DNDeMIjJ67IvWLm1vbmMLaOeCAyjpWO0g9G'
                            },
                            success: function (response) {
                                if (response.status == 'ok') {
                                    alert("Favouri ajouté!");
                                }
                            },
                            error: function (response) {
                                alert("Une erreur est survenue, veuillez réessayer!");
                            }
                        });
                    });
                            </script>
